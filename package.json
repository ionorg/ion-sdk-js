{
  "name": "ion-sdk-js",
  "version": "1.8.2",
  "description": "A js sdk for ion-sfu or ion",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/*",
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "bundle": "webpack",
    "dev": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build && npm run bundle",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "web-dev": "webpack serve --mode development --open --config ./webpack.dev.config.js --hot --inline --port 8081 --host localhost"
  },
  "author": "pion",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/runtime": "7.12.5",
    "@types/events": "3.0.0",
    "@types/google-protobuf": "3.7.4",
    "@types/node": "14.14.14",
    "@types/sdp-transform": "2.4.4",
    "@types/uuid": "8.3.0",
    "prettier": "2.8.7",
    "ts-loader": "8.0.12",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.3",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "^4.44.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-dev-server-output": "0.0.6"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@improbable-eng/grpc-web": "^0.14.1",
    "browser-headers": "^0.4.1",
    "events": "^3.0.0",
    "google-protobuf": "^3.6.1",
    "sdp-transform": "^2.14.0",
    "uuid": "^8.0.0",
    "webpack-cli": "^4.8.0"
  },
  "keywords": [
    "pion",
    "sfu",
    "webrtc",
    "ion",
    "js"
  ]
}
