{
  "name": "ion-sdk-js",
  "version": "1.6.0",
  "description": "A js sdk for ion sfu",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/*",
    "lib/**/*"
  ],
  "scripts": {
    "addProtosToBuild": "[ -d ./lib/signal/_proto/library ] || (mkdir ./lib/signal/_proto && mkdir ./lib/signal/_proto/library) && cp -r ./src/signal/_proto/library ./lib/signal/_proto",
    "build": "tsc && npm run addProtosToBuild",
    "bundle": "webpack",
    "dev": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build && npm run bundle",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "pion",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.8",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-react": "7.12.13",
    "@babel/runtime": "7.13.8",
    "@types/events": "3.0.0",
    "@types/google-protobuf": "3.7.4",
    "@types/node": "14.14.14",
    "@types/sdp-transform": "2.4.4",
    "@types/uuid": "8.3.0",
    "prettier": "2.2.1",
    "ts-loader": "8.0.12",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.3",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "4.44.2",
    "webpack-cli": "4.2.0"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.13.0",
    "events": "^3.0.0",
    "google-protobuf": "^3.6.1",
    "sdp-transform": "^2.14.0",
    "uuid": "^8.0.0"
  },
  "keywords": [
    "pion",
    "sfu",
    "webrtc",
    "ion",
    "js"
  ]
}
